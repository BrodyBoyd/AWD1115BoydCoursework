@model IEnumerable<Trip>
<div>
    <a asp-controller="Trip" asp-action="Add" asp-route-id="1">Add Trip</a>
</div>
<table>
    <thead>
        <tr>
            <th>Destination</th>
            <th>Start Date</th>
            <th>End Date</th>
            <th>Accommodations</th>
            <th>Things To Do</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var trip in Model)
        {
            <tr>
                <td>@trip.Destination</td>
                <td>@trip.StartDate.ToShortDateString()</td>
                <td>@trip.EndDate.ToShortDateString()</td>
                <td>@trip.Accommodation</td>
                <td>
                    <ul>
                        <li>@trip.Activity1</li>
                        <li>@trip.Activity2</li>
                        <li>@trip.Activity3</li>
                    </ul>
                </td>
            </tr>
        }
    </tbody>
</table>